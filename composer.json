{
    "name": "shippi/sdk",
    "license": "proprietary",
    "description": "SDK for Shippii API v4",
    "type": "library",
    "authors": [
        {
            "name": "Stoyan Todorov",
            "email": "s.todorov@vconnect.dk"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.9",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-mock": "^1.0",
        "phpstan/phpstan": "^1.8",
        "symfony/var-dumper": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Shippii\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
