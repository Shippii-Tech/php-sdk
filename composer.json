{
    "name": "shippi/sdk",
    "license": "proprietary",
    "description": "SDK for Shippii API v4",
    "type": "library",
    "authors": [
        {
            "name": "Stoyan Todorov",
            "email": "s.todorov@vconnect.dk"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13.0",
        "phpstan/phpstan": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "Shippii\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
